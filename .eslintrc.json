{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "extends": [
    "eslint-config-ndla-h5p",
    "plugin:jsdoc/recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "plugins": ["jsdoc"],
  "settings": {
    "jsdoc": {
      "preferredTypes": {
        "Function": "function"
      }
    }
  }
}
